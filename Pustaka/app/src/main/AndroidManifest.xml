<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tugas.pustaka">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/gedebug"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gedebug"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pustaka"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.ReadBookActivity"></activity>
        <activity android:name=".view.DestinationActivity" />
        <activity android:name=".view.RecyclerActivity" />
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>